<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <LangVersion>8.0</LangVersion>
  </PropertyGroup>

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <RootNamespace>korsback.dotnet.extensions</RootNamespace>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageId>korsback.dotnet.extensions</PackageId>
    <Author>Oskar K</Author>
    <Product>dotnet extensions</Product>
    <Description>Package to provide some nice to have extensions to use when developing with .NET. Code that otherwize has to be written over and over again for each project.</Description>
    <Copyright>2021</Copyright>
    <PackageProjectUrl>https://github.com/korsback/</PackageProjectUrl>
    <PackageTags>Extensions</PackageTags>
    <PackageReleaseNotes>Update namespaces</PackageReleaseNotes>
    <Version>0.0.0.2</Version>
    <Authors>Oskar K</Authors>
    <Company />
  </PropertyGroup>
</Project>
